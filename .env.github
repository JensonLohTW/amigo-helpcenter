# AMIGO 幫助中心 - GitHub Pages 部署環境配置
# 此文件用於 GitHub Pages 部署時的環境變數

# GitHub Pages 部署標識
GITHUB_PAGES=true

# 應用配置
NEXT_PUBLIC_APP_NAME=AMIGO-HELPCENTER
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_APP_DESCRIPTION=AMIGO 自動販賣機後台系統操作手冊

# 日誌級別 (生產環境使用 INFO)
LOG_LEVEL=INFO

# 功能開關
NEXT_PUBLIC_ENABLE_SEARCH=true
NEXT_PUBLIC_ENABLE_THEME_TOGGLE=true
NEXT_PUBLIC_ENABLE_FEEDBACK=true
